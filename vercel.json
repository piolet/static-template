{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "regions": ["cdg1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Content-Security-Policy",
          "value" : "base-uri 'self' https://cdnjs.cloudflare.com"
        },
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=86400, must-revalidate"
        },
        {
          "key" : "Content-Type",
          "value" : "text/html; charset=utf-8"
        },
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        },
        {
          "key" : "Referrer-Policy",
          "value" : "strict-origin"
        },
        {
          "key" : "Permissions-Policy",
          "value" : "geolocation=(self), microphone=()"
        }
      ]
    },
    {
      "source": "/assets/(.*).svg",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "image/svg+xml"
        },
        {
          "key" : "Vary",
          "value" : "Accept-Encoding"
        }
      ]
    },
    {
      "source": "/assets/(.*).css",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "text/css"
        },
        {
          "key" : "Vary",
          "value" : "Accept-Encoding"
        }
      ]
    },
    {
      "source": "/assets/(.*).js",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "application/javascript"
        },
        {
          "key" : "Vary",
          "value" : "Accept-Encoding"
        }
      ]
    },
    {
      "source": "/assets/(.*).png",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "image/png"
        }
      ]
    },
    {
      "source": "/assets/(.*).jpg",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "image/jpeg"
        }
      ]
    },
    {
      "source": "/(.*).ico",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "image/x-icon"
        }
      ]
    },
    {
      "source": "/(.*).xml",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "text/xml"
        }
      ]
    },
    {
      "source": "/assets/(.*).ttf",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "font/ttf"
        }
      ]
    },
    {
      "source": "/assets/(.*).eot",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "font/eot"
        }
      ]
    },
    {
      "source": "/assets/(.*).woff",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "font/woff"
        }
      ]
    },
    {
      "source": "/assets/(.*).woff2",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "font/woff2"
        }
      ]
    },
    {
      "source": "/assets/(.*).json",
      "headers": [
        {
          "key" : "Content-Type",
          "value" : "application/json"
        },
        {
          "key" : "Vary",
          "value" : "Accept-Encoding"
        }
      ]
    }
  ]
}